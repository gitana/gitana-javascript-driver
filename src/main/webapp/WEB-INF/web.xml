<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<display-name>Gitana Javascript Driver - Test App</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>	
	
    <!-- Gitana Repository Proxy Servlet -->
	<servlet>
		<servlet-name>Gitana Repository Proxy Servlet</servlet-name>
		<servlet-class>org.gitana.web.servlet.GitanaProxyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Gitana Repository Proxy Servlet</servlet-name>
		<url-pattern>/proxy/*</url-pattern>
	</servlet-mapping>
	
</web-app>	